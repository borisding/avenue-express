{
  "name": "avenue-express",
  "version": "1.1.0",
  "description": "Express MVC application starter for getting shit done.",
  "main": "index.js",
  "bin": "avenue.js",
  "scripts": {
    "clean": "rimraf public config/*.json build/webpack/assets.js",
    "config": "node -r esm build/scripts/config.js",
    "webpack": "npm-run-all clean config && webpack --config build/webpack/config.js",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "build:analyze": "cross-env ANALYZE_MODE=enabled npm run build",
    "postinstall": "npm run build",
    "dev": "cross-env NODE_ENV=development npm run webpack",
    "start": "cross-env NODE_ENV=production node .",
    "lint:js": "eslint . --fix",
    "lint:scss": "stylelint **/*.scss --syntax=scss --fix",
    "lint": "npm-run-all lint:scss lint:js",
    "test": "npm run lint:js && cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "rimraf coverage && npm test -- --coverage"
  },
  "keywords": [
    "express",
    "mvc",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/avenue-express.git"
  },
  "verbose": true,
  "engines": {
    "node": "10.x"
  },
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "colors": "^1.3.2",
    "compression": "^1.7.3",
    "consolidate": "^0.15.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "csurf": "^1.9.0",
    "dotenv": "^6.1.0",
    "dotenv-parse-variables": "^0.2.0",
    "esm": "^3.0.84",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "extend": "^3.0.2",
    "helmet": "^3.14.0",
    "hpp": "^0.2.2",
    "http-errors": "^1.7.1",
    "isdev": "^1.0.1",
    "make-promises-safe": "^2.0.0",
    "mkdirp-promise": "^5.0.1",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.1",
    "nunjucks": "^3.1.3",
    "pluralize": "^7.0.0",
    "random-key": "^0.3.2",
    "rimraf": "^2.6.2",
    "sade": "^1.4.1",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^4.1.1",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.2.0",
    "slash": "^2.0.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-webpack-alias": "^2.1.2",
    "chokidar": "^2.0.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "extract-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.5",
    "nodemon-webpack-plugin": "^4.0.3",
    "npm-run-all": "^4.1.3",
    "offline-plugin": "^5.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.7.0",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "stylelint-scss": "^3.3.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "grid": true,
        "flexbox": "no-2009"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-global-assign": 0,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "database",
    "coverage",
    "logs",
    "public/**/*.js"
  ],
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "selector-max-id": 1,
      "selector-class-pattern": null
    }
  },
  "_moduleAliases": {
    "@root": ".",
    "@bin": "bin",
    "@build": "build",
    "@config": "config",
    "@database": "database",
    "@logs": "logs",
    "@public": "public",
    "@utils": "utils",
    "@src": "src",
    "@controllers": "src/controllers",
    "@middlewares": "src/middlewares",
    "@models": "src/models",
    "@views": "src/views",
    "@assets": "src/assets"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/tests/mocks/styleMock.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/mocks/"
    ]
  }
}
