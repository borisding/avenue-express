{
  "name": "avenue-express",
  "version": "4.3.1",
  "description": "Express MVC application starter for getting shit done.",
  "main": "index.js",
  "bin": "avenue.js",
  "scripts": {
    "clean": "rimraf public",
    "webpack": "npm run clean && webpack --progress --hide-modules",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "build:analyze": "cross-env ANALYZE_MODE=enabled npm run build",
    "dev": "cross-env NODE_ENV=development npm run webpack",
    "start": "cross-env NODE_ENV=production node .",
    "lint:js": "eslint . --fix",
    "lint:scss": "stylelint **/*.scss **/*.vue --fix",
    "lint": "npm-run-all lint:scss lint:js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "rimraf storage/coverage && npm test -- --coverage --runInBand",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "express",
    "mvc",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/avenue-express.git"
  },
  "verbose": true,
  "engines": {
    "npm": ">=5.6.0",
    "node": ">=10.0.0"
  },
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "bulma": "^0.8.2",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-hbs": "^2.3.3",
    "express-minify-html-2": "^1.0.1",
    "express-session": "^1.17.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "http-errors": "^1.7.3",
    "isdev": "^1.0.1",
    "make-promises-safe": "^5.1.0",
    "mkdirp": "^1.0.4",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "random-key": "^0.3.2",
    "rimraf": "^3.0.2",
    "sade": "^1.7.3",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "session-file-store": "^1.4.0",
    "slash": "^3.0.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@vue/test-utils": "^1.0.0-beta.33",
    "assets-webpack-plugin": "^3.9.12",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "extract-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon-webpack-plugin": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.17.1",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.1.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@app": "app",
    "@assets": "assets",
    "@controllers": "app/controllers",
    "@middlewares": "app/middlewares",
    "@models": "app/models",
    "@views": "app/views",
    "@config": "config",
    "@logger": "logger",
    "@public": "public",
    "@utils": "utils",
    "@scripts": "scripts",
    "@storage": "storage",
    "@database": "storage/database",
    "@logs": "storage/logs"
  }
}
