{
  "name": "avenue-express",
  "version": "2.2.0",
  "description": "Express MVC application starter for getting shit done.",
  "main": "index.js",
  "bin": "avenue.js",
  "scripts": {
    "clean": "rimraf public app/assets/index.js",
    "webpack": "npm run clean && webpack --progress --hide-modules",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "build:analyze": "cross-env ANALYZE_MODE=enabled npm run build",
    "dev": "cross-env NODE_ENV=development npm run webpack",
    "start": "cross-env NODE_ENV=production node .",
    "lint:js": "eslint . --fix",
    "lint:scss": "stylelint **/*.scss --syntax=scss --fix",
    "lint": "npm-run-all lint:scss lint:js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "rimraf storage/coverage && npm test -- --coverage --runInBand",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "express",
    "mvc",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/avenue-express.git"
  },
  "verbose": true,
  "engines": {
    "npm": ">=5",
    "node": ">=8.3.0"
  },
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "colors": "^1.3.3",
    "compression": "^1.7.3",
    "consolidate": "^0.15.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "csurf": "^1.9.0",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "esm": "^3.1.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "extend": "^3.0.2",
    "helmet": "^3.15.0",
    "hpp": "^0.2.2",
    "http-errors": "^1.7.1",
    "isdev": "^1.0.1",
    "make-promises-safe": "^4.0.0",
    "mkdirp-promise": "^5.0.1",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.4",
    "nunjucks": "^3.1.7",
    "pluralize": "^7.0.0",
    "random-key": "^0.3.2",
    "rimraf": "^2.6.3",
    "sade": "^1.4.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.2.0",
    "slash": "^2.0.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^9.4.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-webpack-alias": "^2.1.2",
    "chokidar": "^2.0.4",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "nodemon-webpack-plugin": "^4.0.7",
    "npm-run-all": "^4.1.5",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-scss": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.6.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "grid": true,
        "flexbox": "no-2009"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-global-assign": 0,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "storage/**/*",
    "public/**/*.js"
  ],
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "selector-max-id": 1,
      "selector-class-pattern": null
    }
  },
  "_moduleAliases": {
    "@root": ".",
    "@app": "app",
    "@assets": "app/assets",
    "@controllers": "app/controllers",
    "@middlewares": "app/middlewares",
    "@models": "app/models",
    "@views": "app/views",
    "@bin": "bin",
    "@config": "config",
    "@logger": "logger",
    "@public": "public",
    "@utils": "utils",
    "@storage": "storage",
    "@database": "storage/database",
    "@logs": "storage/logs"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTest.js"
    ],
    "coverageDirectory": "<rootDir>/storage/coverage",
    "collectCoverageFrom": [
      "app/**/*.js",
      "app/assets/components/**/*.vue",
      "!**/node_modules/**",
      "!app/index.js",
      "!app/assets/index.js"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/config/jest/styleMock.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
