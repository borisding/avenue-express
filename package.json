{
  "name": "avenue-express",
  "version": "5.0.0",
  "description": "Express MVC application starter for getting shit done.",
  "main": "index.js",
  "bin": "avenue.js",
  "scripts": {
    "clean": "rimraf public",
    "webpack": "npm run clean && webpack --progress",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "build:analyze": "cross-env ANALYZE_MODE=enabled npm run build",
    "dev": "cross-env NODE_ENV=development npm run webpack",
    "start": "cross-env NODE_ENV=production node .",
    "lint:js": "eslint . --fix",
    "lint:scss": "stylelint **/*.scss **/*.vue --fix",
    "lint": "npm-run-all lint:scss lint:js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "rimraf storage/coverage && npm test -- --coverage --runInBand",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "express",
    "mvc",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/borisding/avenue-express.git"
  },
  "verbose": true,
  "engines": {
    "node": ">=10.13.0"
  },
  "author": "Boris Ding Poh Hing",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "bulma": "^0.9.1",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eta": "^1.12.1",
    "express": "^4.17.1",
    "express-minify-html-2": "^1.0.1",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "http-errors": "^1.8.0",
    "isdev": "^1.0.1",
    "make-promises-safe": "^5.1.0",
    "mkdirp": "^1.0.4",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.13",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "random-key": "^0.3.2",
    "rimraf": "^3.0.2",
    "sade": "^1.7.4",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "slash": "^3.0.0",
    "vue": "^2.6.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-strict-mode": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@vue/test-utils": "^1.1.2",
    "assets-webpack-plugin": "^6.1.2",
    "autoprefixer": "^10.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "core-js": "^3.8.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "nodemon-webpack-plugin": "^4.4.4",
    "npm-run-all": "^4.1.5",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.9.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpackbar": "^5.0.0-3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@app": "app",
    "@assets": "assets",
    "@controllers": "app/controllers",
    "@middlewares": "app/middlewares",
    "@models": "app/models",
    "@views": "app/views",
    "@config": "config",
    "@logger": "logger",
    "@public": "public",
    "@utils": "utils",
    "@scripts": "scripts",
    "@storage": "storage",
    "@database": "storage/database",
    "@logs": "storage/logs"
  }
}
